{
  "manifest_version": 2,

  "name": "links",
  "description": "This extension provides the first link of the web page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "url.png",
    "default_popup": "index.html",
    "default_title": "shop here"
  },
  "icons": {
    "64": "url.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
"content_scripts": [
  {
    "matches": ["https://*/*"],

    "js": ["jquery-3.2.1.js", "content_script.js"]
  }
]
}
